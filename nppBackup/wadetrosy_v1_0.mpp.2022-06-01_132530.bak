prosol relations=<triple>

#include <Avance.incl>
#include <Grad.incl>
#include <Delay.incl>


"p61 = 50u"
"p62 = p21"
"plw7 = 0"
"plw0 = 0"

"p11=1m"
"spw1 = plw1*pow(p1/((p11/1m)*588.9), 2)"

"spw39=plw1*pow(p1/p61, 2)"
"p39 =p61*25.9"    /* Universal PI/2 pulse  */
"spoff39=0.0"     /* Universal PI/2 pulse offset */ 
;spnam39  wade_pix_rf0.00_25.900p1_bw1.75p1.mrf; (p39:sp39 ph1)



"p31 =p1*7.984"    /* Universal PI/2 pulse  */
"spoff31=0.0"     /* Universal PI/2 pulse offset */
"spw31=plw1"      /* Universal PI/2 pulse power level  */
;spnam31 spnam31 pix_rf0.00_7.984p1_bw0.70p1.mvs

 

"p14 =p3*10.021"    /* Universal PI/2 pulse  */
"spoff3=0.0"     /* Universal PI/2 pulse offset */
"spw3=plw2"      /* Universal PI/2 pulse power level  */
;z2iz_rf0.00_10.021p1_bw2.40B1.mrf;


"spw51=plw3*pow(p21/p62, 2)"
"p51 =p62*4.61"    /* Universal PI/2 pulse  */
"spoff51=0.0"     /* Universal PI/2 pulse offset */ 
;spnam51  z2iz_rf0.00_4.610p1_bw0.80B1.mrf; 

"spw52=plw3*pow(p21/p62, 2)"
"p52 =p62*7.093"    /* Universal PI/2 pulse  */
"spoff52=0.0"     /* Universal PI/2 pulse offset */ 
;spnam52 pix_rf0.00_7.093p1_bw0.80B1.mrf


"p2=p1*2"
"p22=p21*2"
"d11=30m"
"d26=1s/(cnst4*4)"


"d0=6u"

"in0 = inf1/2"
"d51 = p39/2"
"d52 = p52/2"

"DELTA1=d26-p16-d16-4u"
"DELTA2=d26-p16-d16-p11-8u - d52"
"DELTA3=d26-p16-d16-4u-d51 ";"DELTA3=d26-p16-d16-p27*2.385-d19*5-4u"
"DELTA4=d19-p22/2"
"DELTA5=d26-p16-d16-p21-4u-d51 ";"DELTA5=d26-p16-d16-p27*2.154-p0*0.231-d19*5-p21-4u"

#   ifdef LABEL_CN
"DELTA6=p14+d0*2+p21*4/3.1416+4u"
#   else
"DELTA6=d0*2+p21*4/3.1416+2u"
#   endif /*LABEL_CN*/


"l0=1"


1 ze 
2 d11
3 d1 pl1:f1
  50u UNBLKGRAD
  (p1 ph1)
  4u
  p16:gp1
  d16
  DELTA1 pl7:f3 pl0:f1
  (center (p31:sp31 ph21) (p51:sp51 ph1):f3 );(center (p2 ph2) (p22 ph1):f3 )
  4u 
  DELTA1 pl3:f3 pl1:f1
  p16:gp1
  d16
  (p1 ph8) 
  4u pl0:f1
  (p11:sp1 ph6:r):f1
  4u
  p16:gp1
  d16 pl1:f1

  if "l0 %2 == 1"
     {
     (p21 ph3):f3
     }
  else
     {
     (p21 ph4):f3
     }


#   ifdef LABEL_CN
  d0 gron0
  2u groff
  (p14:sp3 ph1):f2
  d0 gron0*-1
  2u groff
#   else
  d0 gron0
  d0 gron0*-1
  2u groff pl7:f3
#   endif /*LABEL_CN*/

 (p52:sp52 ph1):f3 ; (p22 ph1):f3
  DELTA6 pl3:f3

  (p1 ph5)
  4u pl0:f1
  (p11:sp1 ph5:r):f1
  4u
  p16:gp2
  d16
  DELTA2 pl0:f1  pl7:f3
  (center (p31:sp31 ph22)  (p52:sp52 ph1):f3); (center (p2 ph1) (p22 ph1):f3 )
  p16:gp2
  d16
  DELTA2 pl0:f1  pl3:f3
  (p11:sp1 ph6:r):f1
  4u
  4u pl1:f1
  (center (p1 ph1) (p21 ph2):f3 )

  4u
  p16:gp3
  d16
  DELTA3 pl0:f1  pl7:f3 

  (center (p39:sp39 ph1):f1  (p52:sp52 ph1):f3); (center (p39:sp39 ph1):f1  (p52:sp52 ph1):f3 );(center (p39:sp39 ph1):f1  (p22 ph1):f3)

/*
  p27*0.231 ph1
  d19*2
  p27*0.692 ph1
  d19*2
  p27*1.462 ph1
  DELTA4
  (p22 ph1):f3
  DELTA4
  p27*1.462 ph6
  d19*2
  p27*0.692 ph6
  d19*2
  p0*0.231 ph6
*/

  DELTA5
  p16:gp3
  d16  pl18:f1  pl3:f3
  4u BLKGRAD

  (p21 ph7):f3
  go=2 ph31
  d11 mc #0 to 2 F1EA(calph(ph5, +180) & calph(ph7, +180) & calclc(l0, 1), caldel(d0, +in0) & calph(ph3, +180) & calph(ph4, +180) & calph(ph31, +180))
exit 
  

ph1=0
ph2=1
ph3=1 3 2 0
ph4=3 1 2 0
ph5=3
ph6=2
ph7=2
ph8=3
ph31=0 2 3 1

ph21=0
ph22=1

;pl1 : f1 channel - power level for pulse (default)
;pl3 : f3 channel - power level for pulse (default)
;pl18: f1 channel - power level for 3-9-19-pulse (watergate)
;sp3: f2 channel - shaped pulse 180 degree (adiabatic)
;p0 : f1 channel -  90 degree pulse at pl18
;                      use for fine adjustment
;p1 : f1 channel -  90 degree high power pulse
;p2 : f1 channel - 180 degree high power pulse
;p14: f2 channel - 180 degree shaped pulse for inversion (adiabatic)
;p16: homospoil/gradient pulse
;p21: f3 channel -  90 degree high power pulse
;p22: f3 channel - 180 degree high power pulse
;p27: f1 channel -  90 degree pulse at pl18
;d0 : incremented delay (2D)                         [6 usec]
;d1 : relaxation delay; 1-5 * T1
;d11: delay for disk I/O                             [30 msec]
;d16: delay for homospoil/gradient recovery
;d19: delay for binomial water suppression
;     d19 = (1/(2*d)), d = distance of next null (in Hz)
;d26 : 1/(4J)YH
;cnst4: = J(YH)
;inf1: 1/SW(X) = 2 * DW(X)
;in0: 1/(2 * SW(X)) = DW(X)
;nd0: 2
;NS: 4 * n
;DS: 16
;td1: number of experiments
;FnMODE: echo-antiecho


;use gradient ratio:    gp 0 : gp 1 : gp 2 : gp 3
;                          3 :   30 :   45 :   50

;for z-only gradients:
;gpz0: 3%
;gpz1: 30%
;gpz2: 45%
;gpz3: 50%

;use gradient files:
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100
;gpnam3: SMSQ10.100


                                          ;preprocessor-flags-start
;LABEL_CN: for C-13 and N-15 labeled samples start experiment with
;             option -DLABEL_CN (eda: ZGOPTNS)
                                          ;preprocessor-flags-end



